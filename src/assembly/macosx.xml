<?xml version="1.0" encoding="UTF-8"?>
<assembly>
    <id>macosx</id>
    <formats>
        <format>dir</format>
    </formats>
    <moduleSets>
        <moduleSet>
            <includes>
                <include>*:runtime:jar:*</include>
            </includes>
            <binaries>
                <outputDirectory>MyTunesRSS.app/Contents/Java</outputDirectory>
                <outputFileNameMapping>mytunesrss.jar</outputFileNameMapping>
                <unpack>false</unpack>
                <includeDependencies>true</includeDependencies>
                <dependencySets>
                    <dependencySet>
                        <outputDirectory>MyTunesRSS.app/Contents/Java/lib</outputDirectory>
                        <includes>
                            <include>*:jar:*</include>
                        </includes>
                    </dependencySet>
                </dependencySets>
            </binaries>
        </moduleSet>
        <moduleSet>
            <includes>
                <include>*:webapp:war:*</include>
            </includes>
            <binaries>
                <outputDirectory>MyTunesRSS.app/Contents/Java/webapps/ROOT</outputDirectory>
                <unpack>true</unpack>
                <includeDependencies>false</includeDependencies>
            </binaries>
        </moduleSet>
        <moduleSet>
            <includes>
                <include>*:admin:war:*</include>
            </includes>
            <binaries>
                <outputDirectory>MyTunesRSS.app/Contents/Java/webapps/ADMIN</outputDirectory>
                <unpack>true</unpack>
                <includeDependencies>false</includeDependencies>
            </binaries>
        </moduleSet>
    </moduleSets>
    <dependencySets>
        <dependencySet>
            <outputDirectory>MyTunesRSS.app/Contents/Java/lib</outputDirectory>
        </dependencySet>
    </dependencySets>
    <fileSets>
        <fileSet>
            <outputDirectory>MyTunesRSS.app/Contents/Java</outputDirectory>
            <directory>etc</directory>
            <includes>
                <include>themes/**</include>
                <include>languages/**</include>
            </includes>
        </fileSet>
        <fileSet>
            <outputDirectory>.</outputDirectory>
            <directory>etc/MacOS-Bundle</directory>
            <includes>
                <include>**</include>
            </includes>
            <excludes>
                <exclude>MyTunesRSS.app/Contents/MacOS/MyTunesRSS</exclude>
                <exclude>MyTunesRSS.app/Contents/Info.plist</exclude>
            </excludes>
        </fileSet>
        <fileSet>
            <outputDirectory>.</outputDirectory>
            <directory>etc/MacOS-Bundle</directory>
            <includes>
                <include>MyTunesRSS.app/Contents/Info.plist</include>
            </includes>
            <filtered>true</filtered>
        </fileSet>
        <fileSet>
            <outputDirectory>.</outputDirectory>
            <directory>etc/MacOS-Bundle</directory>
            <includes>
                <include>MyTunesRSS.app/Contents/MacOS/MyTunesRSS</include>
            </includes>
            <fileMode>755</fileMode>
        </fileSet>
        <fileSet>
            <outputDirectory>.</outputDirectory>
            <directory>etc</directory>
            <includes>
                <include>readme.*</include>
            </includes>
        </fileSet>
    </fileSets>
</assembly>
