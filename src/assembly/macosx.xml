<?xml version="1.0" encoding="UTF-8"?>
<assembly>
    <id>macosx</id>
    <formats>
        <format>dir</format>
    </formats>
    <moduleSets>
        <moduleSet>
            <binaries>
                <outputDirectory>MyTunesRSS.app/Contents/Resources/Java</outputDirectory>
                <outputFileNameMapping>mytunesrss.jar</outputFileNameMapping>
                <includes>
                    <include>*:runtime:jar:*</include>
                </includes>
            </binaries>
        </moduleSet>
        <moduleSet>
            <binaries>
                <outputDirectory>MyTunesRSS.app/Contents/Resources/Java/webapps</outputDirectory>
                <outputFileNameMapping>ROOT.war</outputFileNameMapping>
                <includes>
                    <include>*:war:*</include>
                </includes>
            </binaries>
        </moduleSet>
    </moduleSets>
    <dependencySets>
        <dependencySet>
            <outputDirectory>MyTunesRSS.app/Contents/Resources/Java/lib</outputDirectory>
            <includes>
                <include>*:jar:*</include>
            </includes>
        </dependencySet>
    </dependencySets>
    <fileSets>
        <fileSet>
            <outputDirectory>.</outputDirectory>
            <directory>etc/MacOS-Bundle</directory>
            <includes>
                <include>**</include>
            </includes>
        </fileSet>
        <fileSet>
            <outputDirectory>MyTunesRSS.app/Contents/Resources/Java/conf</outputDirectory>
            <directory>etc/conf</directory>
            <includes>
                <include>**</include>
            </includes>
        </fileSet>
    </fileSets>
</assembly>
