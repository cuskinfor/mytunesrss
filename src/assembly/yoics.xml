<?xml version="1.0" encoding="UTF-8"?>
<assembly>
    <id>yoics</id>
    <formats>
        <format>dir</format>
        <format>zip</format>
    </formats>
    <moduleSets>
        <moduleSet>
            <includes>
                <include>*:runtime:jar:*</include>
            </includes>
            <binaries>
                <outputDirectory>data</outputDirectory>
                <outputFileNameMapping>mytunesrss.jar</outputFileNameMapping>
                <unpack>false</unpack>
                <includeDependencies>true</includeDependencies>
                <dependencySets>
                    <dependencySet>
                        <outputDirectory>data/lib</outputDirectory>
                        <includes>
                            <include>*:jar:*</include>
                        </includes>
                    </dependencySet>
                </dependencySets>
            </binaries>
        </moduleSet>
        <moduleSet>
            <includes>
                <include>*:webapp:war:*</include>
            </includes>
            <binaries>
                <outputDirectory>data/webapps/ROOT</outputDirectory>
                <unpack>true</unpack>
                <includeDependencies>false</includeDependencies>
            </binaries>
        </moduleSet>
        <moduleSet>
            <includes>
                <include>*:admin:war:*</include>
            </includes>
            <binaries>
                <outputDirectory>data/webapps/ADMIN</outputDirectory>
                <unpack>true</unpack>
                <includeDependencies>false</includeDependencies>
            </binaries>
        </moduleSet>
    </moduleSets>
    <dependencySets>
        <dependencySet>
            <outputDirectory>data/lib</outputDirectory>
        </dependencySet>
    </dependencySets>
    <fileSets>
        <fileSet>
            <outputDirectory>data</outputDirectory>
            <directory>etc</directory>
            <includes>
                <include>themes/**</include>
                <include>languages/**</include>
            </includes>
        </fileSet>
        <fileSet>
            <outputDirectory>.</outputDirectory>
            <directory>etc</directory>
            <includes>
                <include>readme.*</include>
            </includes>
        </fileSet>
        <fileSet>
            <outputDirectory>data</outputDirectory>
            <directory>etc</directory>
            <includes>
                <include>jre/**</include>
            </includes>
        </fileSet>
        <fileSet>
            <outputDirectory>.</outputDirectory>
            <directory>etc/yoics</directory>
            <includes>
                <include>*</include>
            </includes>
        </fileSet>
    </fileSets>
    <files>
        <file>
            <source>etc/MyTunesRSS-jre.exe</source>
            <outputDirectory>.</outputDirectory>
            <destName>MyTunesRSS.exe</destName>
        </file>
    </files>
</assembly>
